{
  "presets": [
    ["babel-preset-env", {
      "targets": {
        "node": "6.8",
        "browser": [
          "last 2 versions",
          "safari >= 7",
          "ie >= 11"
        ]
      },
      "es2015": {
        "modules": false
      }
    }],
    "babel-preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    "babel-plugin-syntax-dynamic-import",
    "babel-plugin-transform-object-rest-spread",
    ["babel-plugin-module-resolver", {
      "root": ["./"]
    }]
  ]
}
